{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "marlabs-covid-reporting-adf"
		},
		"MarlabsAzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'MarlabsAzureDatabricks'"
		},
		"MarlabsCovidDlLs_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'MarlabsCovidDlLs'"
		},
		"MarlabsCovidSaLs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MarlabsCovidSaLs'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"MarlabsCovidDlLs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://marlabscoviddl.dfs.core.windows.net/"
		},
		"Marlabs_http_ecdc_europe_euLs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/pamuba/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "marlabscovidsqlserver.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "marlabsadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "marlabs-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MarlabsAzureDatabricks_accessToken": {
						"value": "[parameters('MarlabsAzureDatabricks_accessToken')]"
					},
					"MarlabsCovidDlLs_accountKey": {
						"value": "[parameters('MarlabsCovidDlLs_accountKey')]"
					},
					"MarlabsCovidSaLs_connectionString": {
						"value": "[parameters('MarlabsCovidSaLs_connectionString')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"MarlabsCovidDlLs_properties_typeProperties_url": {
						"value": "[parameters('MarlabsCovidDlLs_properties_typeProperties_url')]"
					},
					"Marlabs_http_ecdc_europe_euLs_properties_typeProperties_url": {
						"value": "[parameters('Marlabs_http_ecdc_europe_euLs_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "marlabs-covid-reporting-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'marlabs-covid-reporting-adf_ArmTemplate_0')]"
			]
		}
	]
}